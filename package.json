{
  "name": "builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:prod": "vite preview --host",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "pre-commit": "npm run lint",
    "create-component": "node create-onaim-component/index.js",
    "serve:compressed": "npm run build && cd server && node server.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.80.6",
    "builder-settings-types": "^0.0.169",
    "language-management-lib": "^1.0.3",
